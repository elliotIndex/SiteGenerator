<!DOCTYPE html>
<html lang="en">

<head>

  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="description" content="">
  <meta name="author" content="">

  <title>{{ tab-title.value }}</title>

  <!-- Bootstrap Core CSS -->
  <link href="vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

  <!-- Custom Fonts -->
  <link href="vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
  <link href='https://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800' rel='stylesheet' type='text/css'>
  <link href='https://fonts.googleapis.com/css?family=Merriweather:400,300,300italic,400italic,700,700italic,900,900italic' rel='stylesheet' type='text/css'>

  <!-- Plugin CSS -->
  <link href="vendor/magnific-popup/magnific-popup.css" rel="stylesheet">
  <link href="creative.min.css" rel="stylesheet" type="text/css">


  {{#editable}}
  <!-- Dynamically generate page CSS from LESS -->
  <!-- This will re-render when user changes primary theme -->
  <link href="creative.less" rel="stylesheet/less" type="text/css">
  <script src="https://cdnjs.cloudflare.com/ajax/libs/less.js/2.7.2/less.min.js" type="text/javascript"></script>

  <!-- Template CSS -->
  <link href="template.min.css" rel="stylesheet">
  {{/editable}}

  <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
  <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
  <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>

<body id="page-top">

  <nav id="mainNav" class="navbar navbar-default navbar-fixed-top">
    <div class="container-fluid">
      <!-- Brand and toggle get grouped for better mobile display -->
      <div class="navbar-header">
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse"  data-target="#bs-example-navbar-collapse-1">
            <span class="sr-only">Toggle navigation</span><i class="fa fa-bars"></i>
        </button>
        <a class="navbar-brand page-scroll" href="#page-top" data-template-path="{{header.title.path}}" target="_parent">
          {{ header.title.value }}
        </a>
      </div>

      <!-- Collect the nav links, forms, and other content for toggling -->
      <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
        <ul class="nav navbar-nav navbar-right">
          {{#section-1.header-link}}
            <li>
              <a class="page-scroll" href="#section-1" data-template-path="{{section-1.header-link.path}}">
                {{ section-1.header-link.value }}
              </a>
           </li>
          {{/section-1.header-link}}

          {{#section-2.header-link}}
            <li>
              <a class="page-scroll" href="#section-2" data-template-path="{{section-2.header-link.path}}">
                {{ section-2.header-link.value }}
              </a>
            </li>
          {{/section-2.header-link}}

          {{#section-3.header-link}}
            <li>
              <a class="page-scroll" href="#section-3" data-template-path="{{section-3.header-link.path}}">
                {{ section-3.header-link.value }}
              </a>
            </li>
          {{/section-3.header-link}}

          {{#section-4.header-link}}
            <li>
              <a class="page-scroll" href="#section-4" data-template-path="{{section-4.header-link.path}}">
                {{ section-4.header-link.value }}
              </a>
            </li>
          {{/section-4.header-link}}

          {{#section-form.header-link}}
            <li>
              <a class="page-scroll" href="#section-form" data-template-path="{{section-form.header-link.path}}">
                {{ section-form.header-link.value }}
              </a>
            </li>
          {{/section-form.header-link}}
        </ul>
      </div>
      <!-- /.navbar-collapse -->
    </div>
    <!-- /.container-fluid -->
  </nav>

  {{#main}}
    <header id="{{img.path}}" style="background-image: linear-gradient( rgba(0, 0, 0, {{darkness.value}}), rgba(0, 0, 0, {{darkness.value}}) ), url('{{{ img.value }}}')">
      <div class="header-content">
        <div class="header-content-inner">
          <h1 id="homeHeading" data-template-path="{{title.path}}">{{ title.value }}</h1>
          <hr>
          <p data-template-path="{{subtitle.path}}">{{ subtitle.value }}</p>
          <a id="{{button-link.path}}" href="{{{ button-link.value }}}" class="btn btn-primary btn-xl page-scroll" data-template-path="{{button.path}}" target="_parent">
            {{ button.value }}
          </a>
          {{#editable}}
          <div class="sg-hidden-template">
            Button Destination Link:
            <span class="sg-external-modifier" data-template-path="{{button-link.path}}">
              {{{ button-link.value }}}
            </span>
          </div>
          {{/editable}}
        </div>
      </div>
      {{#editable}}
      <div class="sg-hidden-template background-img-src">
        Background Image Source:
        <span class="sg-external-modifier" data-template-path="{{img.path}}"> {{{ img.value }}} </span>
      </div>
      {{/editable}}
    </header>
  {{/main}}

  {{#section-1}}
  <section class="bg-primary" id="section-1">
    <div class="container">
      <div class="row">
        <div class="col-lg-8 col-lg-offset-2 text-center">
          <h2 class="section-heading" data-template-path="{{title.path}}">
            {{ title.value }}
          </h2>
          <hr class="light">
          <p class="text-faded" data-template-path="{{text.path}}">
            {{ text.value }}
          </p>
          <a id="{{button-link.path}}" href="{{{ button-link.value }}}" class="page-scroll btn btn-default btn-xl sr-button" data-template-path="{{button.path}}" target="_parent">
            {{ button.value }}
          </a>
          {{#editable}}
          <div class="sg-hidden-template">
            Button Destination Link:
            <span class="sg-external-modifier" data-template-path="{{button-link.path}}"> {{{ button-link.value }}} </span>
          </div>
          {{/editable}}
          {{#editable}}
          <div class="sg-hidden-template">
            Theme Color:
            <span class="sg-external-modifier" data-template-path="{{theme-color.path}}"> {{{ theme-color.value }}} </span>
          </div>
          {{/editable}}
        </div>
      </div>
    </div>
  </section>
  {{/section-1}}

  {{#section-2}}
  <section id="section-2">
    <div class="container">
      <div class="row">
        <div class="col-lg-12 text-center">
          <h2 class="section-heading" data-template-path="{{title.path}}">{{ title.value }}</h2>
          <hr class="primary">
        </div>
      </div>
    </div>
    <div class="container">
      <div class="row">

        {{#blobs}}
        <div class="col-sm-4 col-xs-12 text-center">
          <div class="service-box">
            {{#editable}}
            <div class="sg-hidden-template">
              Icon Type:
              <span class="sg-external-modifier" data-template-path="{{icon.path}}"> {{ icon.value}} </span>
            </div>
            {{/editable}}
            <i id="{{icon.path}}" class="fa fa-4x fa-{{ icon.value }} text-primary sr-icons"></i>
            <h3 data-template-path="{{title.path}}">{{ title.value }}</h3>
            <p class="text-muted" data-template-path="{{text.path}}">{{ text.value }}</p>
          </div>
        </div>
        {{/blobs}}

      </div>
    </div>
  </section>
  {{/section-2}}

  {{#section-3}}
  <section class="no-padding" id="section-3">
    <div class="container-fluid">
      <div class="row no-gutter popup-gallery">

        {{#blobs}}
        <div class="col-lg-4 col-sm-6">
          <div id="{{link.path}}" class="portfolio-box" target="_parent">
            <img id="{{img.path}}" src="{{{ img.value }}}" class="img-responsive" alt="">
            <div class="portfolio-box-caption">
              <div class="portfolio-box-caption-content">
                <div class="project-category text-faded" data-template-path="{{title.path}}">
                  {{ title.value }}
                </div>
                <div class="project-name" data-template-path="{{subtitle.path}}">
                  {{ subtitle.value }}
                </div>
                {{#editable}}
                <div class="sg-hidden-template">
                  Background Image Source:
                  <span class="sg-external-modifier" data-template-path="{{img.path}}"> {{{ img.value }}} </span>
                </div>
                {{/editable}}
                {{#editable}}
                <div class="sg-hidden-template">
                  Destination Link:
                  <span class="sg-external-modifier" data-template-path="{{link.path}}"> {{{ link.value }}} </span>
                </div>
                {{/editable}}
              </div>
            </div>
          </div>
        </div>
        {{/blobs}}

      </div>
    </div>
  </section>
  {{/section-3}}

  {{#section-4}}
  <aside id="section-4" class="bg-dark">
    <div class="container text-center">
      <div class="call-to-action">
        <h2 data-template-path="{{title.path}}">{{ title.value }}</h2>
        <p class="text-muted" data-template-path="{{subtitle.path}}">{{ subtitle.value }}</p>
        {{#button}}
        <a id="button-link.path" href="{{{ button-link.value }}}" class="btn btn-default btn-xl sr-button page-scroll" data-template-path="{{text.path}}" target="_parent" target="_parent">
          {{ text.value }}
        </a>
        {{#editable}}
        <div class="sg-hidden-template">
          Button Destination Link:
          <span class="sg-external-modifier" data-template-path="{{button-link.path}}"> {{{ button-link.value }}} </span>
        </div>
        {{/editable}}
        {{/button}}
      </div>
    </div>
  </aside>
  {{/section-4}}

  {{#section-form}}

  <section id="section-form">
    <div class="container">
      <div class="center-block text-center">
        <h2 class="section-heading" data-template-path="{{title.path}}"> {{ title.value }}</h2>
        <hr class="primary">
        <p data-template-path="{{subtitle.path}}">{{ subtitle.value }}</p>
      </div>
    </div>
    <div class="container">
      <div class="row">
        <form name="signup" method="POST" netlify>

          <!-- Profile -->
          <div class="form-row">
            <div class="form-group">
              <h3 class="col-xs-12">Profile</h3>
            </div>
          </div>
          <div class="form-row">
            <div class="form-group col-sm-6">
              <label for="first-name">First Name</label>
              <input required type="text" name="first-name" class="form-control">
            </div>
            <div class="form-group col-sm-6">
              <label for="last-name">Last Name</label>
              <input required type="text" name="last-name" class="form-control">
            </div>
          </div>
          <div class="form-row">
            <div class="form-group col-sm-6">
              <label for="phone">Phone</label>
              <input required type="phone" name="phone" class="form-control">
            </div>
            <div class="form-group col-sm-6">
              <label for="email">Email</label>
              <input required type="email" name="email" class="form-control">
            </div>
          </div>

          <!-- Shipping -->
          <div class="form-row">
            <div class="form-group">
              <h3 class="col-sm-12">Shipping Address</h3>
            </div>
          </div>
          <div class="form-row">
            <div class="form-group col-sm-12">
              <label for="street-address-1">Street Address</label>
              <input required type="text" name="street-address-1" class="form-control">
            </div>
          </div>
          <div class="form-row">
            <div class="form-group col-sm-12">
              <label for="street-address-2">Address Line 2</label>
              <input type="text" name="street-address-2" class="form-control">
            </div>
          </div>
          <div class="form-row">
            <div class="form-group col-sm-7">
              <label for="city">City</label>
              <input required type="text" name="city" class="form-control">
            </div>
            <div class="form-group col-sm-2">
              <label for="state">State</label>
              <select required name="state" class="form-control">
                <option label=" "></option>
                <option value="AL">Alabama</option>
              	<option value="AK">Alaska</option>
              	<option value="AZ">Arizona</option>
              	<option value="AR">Arkansas</option>
              	<option value="CA">California</option>
              	<option value="CO">Colorado</option>
              	<option value="CT">Connecticut</option>
              	<option value="DE">Delaware</option>
              	<option value="DC">District Of Columbia</option>
              	<option value="FL">Florida</option>
              	<option value="GA">Georgia</option>
              	<option value="HI">Hawaii</option>
              	<option value="ID">Idaho</option>
              	<option value="IL">Illinois</option>
              	<option value="IN">Indiana</option>
              	<option value="IA">Iowa</option>
              	<option value="KS">Kansas</option>
              	<option value="KY">Kentucky</option>
              	<option value="LA">Louisiana</option>
              	<option value="ME">Maine</option>
              	<option value="MD">Maryland</option>
              	<option value="MA">Massachusetts</option>
              	<option value="MI">Michigan</option>
              	<option value="MN">Minnesota</option>
              	<option value="MS">Mississippi</option>
              	<option value="MO">Missouri</option>
              	<option value="MT">Montana</option>
              	<option value="NE">Nebraska</option>
              	<option value="NV">Nevada</option>
              	<option value="NH">New Hampshire</option>
              	<option value="NJ">New Jersey</option>
              	<option value="NM">New Mexico</option>
              	<option value="NY">New York</option>
              	<option value="NC">North Carolina</option>
              	<option value="ND">North Dakota</option>
              	<option value="OH">Ohio</option>
              	<option value="OK">Oklahoma</option>
              	<option value="OR">Oregon</option>
              	<option value="PA">Pennsylvania</option>
              	<option value="RI">Rhode Island</option>
              	<option value="SC">South Carolina</option>
              	<option value="SD">South Dakota</option>
              	<option value="TN">Tennessee</option>
              	<option value="TX">Texas</option>
              	<option value="UT">Utah</option>
              	<option value="VT">Vermont</option>
              	<option value="VA">Virginia</option>
              	<option value="WA">Washington</option>
              	<option value="WV">West Virginia</option>
              	<option value="WI">Wisconsin</option>
              	<option value="WY">Wyoming</option>
              </select>
            </div>
            <div class="form-group col-sm-3">
              <label for="zip-code">Zip Code</label>
              <input required type="text" name="zip-code" class="form-control">
            </div>
          </div>

          <!-- Billing -->
          <div class="form-row">
            <div class="form-group">
              <h3 class="col-sm-12">Billing Info</h3>
            </div>
          </div>
          <div class="form-row">
            <div class="form-group col-sm-6">
              <label for="billing-name">Name on Card</label>
              <input required type="text" name="billing-name" class="form-control">
            </div>
          </div>
          <div class="form-row">
            <div class="form-group col-sm-6">
              <label for="billin-card-num">Card Number</label>
              <input required type="text" name="billin-card-num" class="form-control">
            </div>
          </div>
          <div class="form-row">
            <div class="form-group col-sm-3">
              <label for="billing-exp-month">Expiration Month</label>
              <select required name="billing-exp-month" class="form-control">
                <option label=" "></option>
                <option value="01">01</option>
                <option value="02">02</option>
                <option value="03">03</option>
                <option value="04">04</option>
                <option value="05">05</option>
                <option value="06">06</option>
                <option value="07">07</option>
                <option value="08">08</option>
                <option value="09">09</option>
                <option value="10">10</option>
                <option value="11">11</option>
                <option value="12">12</option>
              </select>
            </div>
            <div class="form-group col-sm-5">
              <label for="billing-exp-year">Year</label>
              <select required name="billing-exp-year" class="form-control">
                <option label=" "></option>
                <option value="2018">2018</option>
                <option value="2019">2019</option>
                <option value="2020">2020</option>
                <option value="2021">2021</option>
                <option value="2022">2022</option>
                <option value="2023">2023</option>
                <option value="2024">2024</option>
                <option value="2025">2025</option>
                <option value="2026">2026</option>
                <option value="2027">2027</option>
                <option value="2028">2028</option>
              </select>
            </div>
            <div class="form-group col-sm-4">
              <label for="billing-security-code">Security Code</label>
              <input required type="number" name="billing-security-code" class="form-control">
            </div>
          </div>
          <div class="form-row text-center">
            <button type="submit" class="btn btn-primary btn-xl">Submit</button>
          <div>
          </div>
        </form>
      </div>
    </div>
  </section>
  {{/section-form}}

  {{#section-5}}
  <section id="section-5" class="bg-dark">
    <div class="container">
      <div class="row">
        <div class="center-block text-center">
          <h2 class="section-heading" data-template-path="{{title.path}}"> {{ title.value }}</h2>
          <hr class="light">
          <p class="text-faded" data-template-path="{{subtitle.path}}">{{ subtitle.value }}</p>
        </div>

        {{#contacts}}
        <div class="col-sm-4 text-center service-box">
          {{#editable}}
          <div class="sg-hidden-template">
            Icon Type:
            <span class="sg-external-modifier" data-template-path="{{icon.path}}"> {{ icon.value}} </span>
          </div>
          {{/editable}}
          <i id="{{icon.path}}" class="fa fa-{{ icon.value }} fa-3x sr-contact"></i>
          <p class="text-faded" data-template-path="{{text.path}}">{{{ text.value }}}</p>
        </div>
        {{/contacts}}
      </div>
    </div>
  </section>
  {{/section-5}}

  <!-- jQuery -->
  <script src="vendor/jquery/jquery.min.js"></script>

  <!-- Bootstrap Core JavaScript -->
  <script src="vendor/bootstrap/js/bootstrap.min.js"></script>

  <!-- Plugin JavaScript -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js"></script>
  <script src="vendor/scrollreveal/scrollreveal.min.js"></script>
  <script src="vendor/magnific-popup/jquery.magnific-popup.min.js"></script>

  <!-- Theme JavaScript -->
  <script src="creative.min.js"></script>

</body>

</html>
