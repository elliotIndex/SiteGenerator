<!DOCTYPE html>
<html lang="en">

<head>

  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="description" content="">
  <meta name="author" content="">

  <title>{{ tab-title.value }}</title>

  <!-- Bootstrap Core CSS -->
  <link href="vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

  <!-- Custom Fonts -->
  <link href="vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
  <link href='https://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800' rel='stylesheet' type='text/css'>
  <link href='https://fonts.googleapis.com/css?family=Merriweather:400,300,300italic,400italic,700,700italic,900,900italic' rel='stylesheet' type='text/css'>

  <!-- Plugin CSS -->
  <link href="vendor/magnific-popup/magnific-popup.css" rel="stylesheet">

  <!-- Theme CSS -->
  <link href="creative.min.css" rel="stylesheet">

  {{#editable}}
  <!-- Template CSS -->
  <link href="template.min.css" rel="stylesheet">
  {{/editable}}

  <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
  <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
  <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>

<body id="page-top">

  <nav id="mainNav" class="navbar navbar-default navbar-fixed-top">
    <div class="container-fluid">
      <!-- Brand and toggle get grouped for better mobile display -->
      <div class="navbar-header">
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse"  data-target="#bs-example-navbar-collapse-1">
            <span class="sr-only">Toggle navigation</span> Menu <i class="fa fa-bars"></i>
        </button>
        <a class="navbar-brand page-scroll" href="#page-top" data-template-path="{{header.title.path}}" target="_parent">
          {{ header.title.value }}
        </a>
      </div>

      <!-- Collect the nav links, forms, and other content for toggling -->
      <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
        <ul class="nav navbar-nav navbar-right">
          {{#section-1.header-link}}
            <li>
              <a class="page-scroll" href="#section-1" data-template-path="{{section-1.header-link.path}}">
                {{ section-1.header-link.value }}
              </a>
           </li>
          {{/section-1.header-link}}

          {{#section-2.header-link}}
            <li>
              <a class="page-scroll" href="#section-2" data-template-path="{{section-2.header-link.path}}">
                {{ section-2.header-link.value }}
              </a>
            </li>
          {{/section-2.header-link}}

          {{#section-3.header-link}}
            <li>
              <a class="page-scroll" href="#section-3" data-template-path="{{section-3.header-link.path}}">
                {{ section-3.header-link.value }}
              </a>
            </li>
          {{/section-3.header-link}}

          {{#section-5.header-link}}
            <li>
              <a class="page-scroll" href="#section-5" data-template-path="{{section-5.header-link.path}}">
                {{ section-5.header-link.value }}
              </a>
            </li>
          {{/section-5.header-link}}
        </ul>
      </div>
      <!-- /.navbar-collapse -->
    </div>
    <!-- /.container-fluid -->
  </nav>

  {{#main}}
    <header style="background-image: linear-gradient( rgba(0, 0, 0, 0.3), rgba(0, 0, 0, 0.3) ), url('{{{ img.value }}}')">
      <div class="header-content">
        <div class="header-content-inner">
          <h1 id="homeHeading" data-template-path="{{title.path}}">{{ title.value }}</h1>
          <hr>
          <p data-template-path="{{subtitle.path}}">{{ subtitle.value }}</p>
          <a href="{{{ button-link.value }}}" class="btn btn-primary btn-xl page-scroll"
            data-template-path="{{button.path}}" target="_parent">
            {{ button.value }}
          </a>
          {{#editable}}
          <div class="sg-hidden-template" data-template-path="{{button-link.path}}">
            Button Destination Link:
            <span data-template-path="{{img.path}}"> {{{ button-link.value }}} </span>
          </div>
          {{/editable}}
        </div>
      </div>
      {{#editable}}
      <div class="sg-hidden-template background-img-src">
        Background Image Source:
        <span data-template-path="{{img.path}}"> {{{ img.value }}} </span>
      </div>
      {{/editable}}
    </header>
  {{/main}}

  {{#section-1}}
  <section class="bg-primary" id="section-1">
    <div class="container">
      <div class="row">
        <div class="col-lg-8 col-lg-offset-2 text-center">
          <h2 class="section-heading" data-template-path="{{title.path}}">{{ title.value }}</h2>
          <hr class="light">
          <p class="text-faded" data-template-path="{{text.path}}">{{ text.value }}</p>
          <a href="{{{ button-link.value }}}" class="page-scroll btn btn-default btn-xl sr-button" data-template-path="{{button.path}}" target="_parent">
            {{ button.value }}
          </a>
          {{#editable}}
          <div class="sg-hidden-template">
            Button Destination Link:
            <span data-template-path="{{button-link.path}}"> {{{ button-link.value }}} </span>
          </div>
          {{/editable}}
        </div>
      </div>
    </div>
  </section>
  {{/section-1}}

  {{#section-2}}
  <section id="section-2">
    <div class="container">
      <div class="row">
        <div class="col-lg-12 text-center">
          <h2 class="section-heading" data-template-path="{{title.path}}">{{ title.value }}</h2>
          <hr class="primary">
        </div>
      </div>
    </div>
    <div class="container">
      <div class="row">

        {{#blobs}}
        <div class="col-lg-3 col-md-6 text-center">
          <div class="service-box">
            {{#editable}}
            <div class="sg-hidden-template">
              Icon Type:
              <span data-template-path="{{icon.path}}"> {{ icon.value}} </span>
            </div>
            {{/editable}}
            <i class="fa fa-4x fa-{{ icon.value }} text-primary sr-icons"></i>
            <h3 data-template-path="{{title.path}}">{{ title.value }}</h3>
            <p class="text-muted" data-template-path="{{text.path}}">{{ text.value }}</p>
          </div>
        </div>
        {{/blobs}}

      </div>
    </div>
  </section>
  {{/section-2}}

  {{#section-3}}
  <section class="no-padding" id="section-3">
    <div class="container-fluid">
      <div class="row no-gutter popup-gallery">

        {{#blobs}}
        <div class="col-lg-4 col-sm-6">
          <a href="{{{ link.value }}}" class="portfolio-box" target="_parent">
            <img src="{{{ img.value }}}" class="img-responsive" alt="">
            <div class="portfolio-box-caption">
              <div class="portfolio-box-caption-content">
                <div class="project-category text-faded" data-template-path="{{title.path}}">
                  {{ title.value }}
                </div>
                <div class="project-name" data-template-path="{{subtitle.path}}">
                  {{ subtitle.value }}
                </div>
                {{#editable}}
                <div class="sg-hidden-template">
                  Background Image Source:
                  <span data-template-path="{{img.path}}"> {{{ img.value }}} </span>
                </div>
                {{/editable}}
                {{#editable}}
                <div class="sg-hidden-template">
                  Destination Link:
                  <span data-template-path="{{link.path}}"> {{{ link.value }}} </span>
                </div>
                {{/editable}}
              </div>
            </div>
          </a>
        </div>
        {{/blobs}}

      </div>
    </div>
  </section>
  {{/section-3}}

  {{#section-4}}
  <aside class="bg-dark">
    <div class="container text-center">
      <div class="call-to-action">
        <h2 data-template-path="{{title.path}}">{{ title.value }}</h2>
        {{#button}}
        <a href="{{{ link.value }}}" class="btn btn-default btn-xl sr-button" data-template-path="{{text.path}}" target="_parent" target="_parent">
          {{ text.value }}
        </a>
        {{#editable}}
        <div class="sg-hidden-template">
          Button Destination Link:
          <span data-template-path="{{link.path}}"> {{{ link.value }}} </span>
        </div>
        {{/editable}}
        {{/button}}
      </div>
    </div>
  </aside>
  {{/section-4}}

  {{#section-5}}
  <section id="section-5">
    <div class="container">
      <div class="row">
        <div class="center-block text-center">
          <h2 class="section-heading" data-template-path="{{title.path}}"> {{ title.value }}</h2>
          <hr class="primary">
          <p data-template-path="{{subtitle.path}}">{{ subtitle.value }}</p>
        </div>

        {{#contacts}}
        <div class="col-lg-4 text-center">
          {{#editable}}
          <div class="sg-hidden-template">
            Icon Type:
            <span data-template-path="{{icon.path}}"> {{ icon.value}} </span>
          </div>
          {{/editable}}
          <i class="fa fa-{{ icon.value }} fa-3x sr-contact"></i>
          <p data-template-path="{{text.path}}">{{{ text.value }}}</p>
        </div>
        {{/contacts}}

      </div>
    </div>
  </section>
  {{/section-5}}

  <!-- jQuery -->
  <script src="vendor/jquery/jquery.min.js"></script>

  <!-- Bootstrap Core JavaScript -->
  <script src="vendor/bootstrap/js/bootstrap.min.js"></script>

  <!-- Plugin JavaScript -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js"></script>
  <script src="vendor/scrollreveal/scrollreveal.min.js"></script>
  <script src="vendor/magnific-popup/jquery.magnific-popup.min.js"></script>

  <!-- Theme JavaScript -->
  <script src="creative.min.js"></script>

</body>

</html>
